body, div, h1, h2, dl, dd, ul, input {
	margin: 0;
	padding: 0;
	border: 0;
}

*, *:before, *:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box; 
    -ms-box-sizing: border-box;
    box-sizing: border-box;
}

ol, ul {
	list-style: none;
}

html {
	overflow: auto;
}

body {
	background-color: #ebf0f6;
	min-width: 510px;
	_width: expression(( document.documentElement.clientWidth || 
		 document.body.clientWidth)>510?"510px":"");
	background-image: url("../images/bg.png");
}

body, button, input, select, textarea {
	color: #3c3c3c;
	font-size: 14px;
	font-family: Verdana, Tahoma, Arial, 'Helvetica Neue', Helvetica, 微软雅黑;
}

a, .h3-info {
	font-size: 14px!important;
	text-decoration: none;
}

img.cache, .info-hide {
	display: none!important;
}

.line-height-45 {
	line-height: 45px!important;
}

.text-left {
	text-align: left;
}

.text-center {
	text-align: center;
}

.text-right {
	text-align: right;
}

.font-default {
	color: #008194;
}

.font-name {
	color: #FF6A00;
}

.font-red {
	color: #FF0000;
}

.font-blue {
	color: #0000FF;
}

.font-green {
	color: #008000;
}

.no-margin {
	margin: 0!important;
}

img {
	border: none;
}

select.form-control {
	_font-size: 24px;
}

.form-control {
	height: 34px;
	height: 22px\9;
}

.form-control:focus {
	border-color: #0097CF;
	outline: 0;
	-moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px
		rgba(0, 151, 207, 0.6);
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px
		rgba(0, 151, 207, 0.6);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px
		rgba(0, 151, 207, 0.6);
}

.input-group-btn {
    position: relative;
    white-space: nowrap;
}

.input-group-addon:first-child {
	border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

.input-group-addon.letter4-3 {
	letter-spacing: 0.5em;
	margin-right: -0.5em;
}

.input-group-addon {
	margin: 0;
    padding: 6px 12px;
    text-align: center;
    background-color: #eee;
    border: 1px solid #ACA7A7;
	border-radius: 4px 0 0 4px;
    font-size: 15px;
    color: #393939;
    height: 20px\9;
    line-height: 20px\9;
    border-right: 0;
    border-right: 0\9;
}

.input-group-addon, .input-group-btn {
    vertical-align: middle;
}

.input-group .form-control:not(:first-child):not(:last-child) {
    border-radius: 0;
}

.input-group-btn .btn {
	margin: 0;
    font-size: 15px;
	padding: 7px 12px;
    border-radius: 0 4px 4px 0;
    height: 20px\9;
    line-height: 20px\9;
}

dd {
	padding: 5px!important;
}

.lab {
	width: 150px;
	float: left;
	padding: 5px 8px;
	text-align: right;
	color: #000;
}

.lab, dd b {
	font-size: 14px!important;
}

.btn {
	background-color: #008194;
	color: #fff;
	border-radius: 4px;
	padding: 6px 12px;
	display: inline-block;
	margin-right: 15px;
}

.btn:HOVER {
	background-color: #2196F3;
}

.btn-h-red:HOVER {
	background-color: #FF0000 !important;
}

.font-error {
	color: #FF0000
}

.header {
	position: fixed;
	width: 100%;
	top: 0;
	left: 0;
	z-index: 1;
	background-color: #008194;
	-moz-box-shadow: 0 4px 8px #517175;
	-webkit-box-shadow: 0 4px 8px #517175;
	box-shadow: 0 4px 8px #517175;
	*filter: progid:DXImageTransform.Microsoft.Shadow(color='#517175',
		Direction=180, Strength=5);
}

.header .content, .container, .foot .content, .info {
	width: 70%;
	margin: 0 auto;
}

.header .content {
	height: 60px;
	background-image: url(../images/web/logo.png);
	background-repeat: no-repeat;
	background-position: left center;
	background-size: 270px 60px;
	position: relative;
}

.header .tools {
	float: right;
	height: 60px;
	line-height: 60px;
	margin-right: 10px;
}

.header .tools a {
	color: #FFF;
}

.header .tools a:HOVER {
	color: #DEDEDE;
	text-decoration: underline;
}

.header .content, .container, .info {
    max-width: 1080px;
	_width: expression(( document.documentElement.clientWidth || 
		 document.body.clientWidth)>1080?"1080px":"");
}

.container {
	position: relative;
    top: 80px;
    color: #008194;
    _margin-top: -50px;
}

.center {
	border: solid 1px #D7E5FF;
	background-color: #F9FAFC;
	-moz-box-shadow: 3px 3px 4px #ccc;
	-webkit-box-shadow: 3px 3px 4px #ccc;
	box-shadow: 3px 3px 4px #ccc;
}

.info-img {
	position: relative;
	top: 3px;
	left: 3px;
}

.center .center-left {
	width: 228px;
	float: left;
}

.center .center-right {
	border-left: 2px solid #E6EEFD;
	background-color: #FFFFFF;
	min-height: 380px;
	display: inline-block;
	width: calc(100% - 230px);
	_width: expression(( document.documentElement.clientWidth || 
		 document.body.clientWidth)<750?"750px":"");
	_height: expression(( document.documentElement.clientHeight || 
		 document.body.clientHeight)>380?"380px":"");
}

.center .center-right .content {
	width: 90%;
	margin: 0 auto;
    padding: 20px 0px;
}

.center .center-right .content fieldset {
	margin: 10px auto;
	border: 2px solid #008194;
	padding: 0;
}

.center .center-right .content ul {
	margin: 20px 15px;
	text-align: center;
}

.center .center-right .content ul li a {
	margin: 5px 10px;
	display: inline-block;
	zoom: 1;
	color: #FFFFFF;
	*cursor: pointer;
	*display: inline;
	line-height: 30px;
	padding: 5px 10px;
	background-color: #1AA9BD;
	border-radius: 2px;
}

.center .center-right .content ul li a:HOVER {
	background-color: #0098AF;
}

.center .center-right .content fieldset legend {
	width: 90%;
	text-align: center;
	margin: 0 auto;
	color: #008194;
	letter-spacing: 2px;
	font-size: 16px;
	font-weight: 500;
	word-break: break-all;
	display: table-row;
}

.center .center-left .careful li {
	line-height: 24px;
	margin: 10px 10px 0;
}

.center .center-left .left-title {
	background-image: url(../images/title.png);
	text-indent: 28px;
	color: #fff;
	font-size: 14px;
	font-weight: 700;
	line-height: 26px;
	width: 194px;
	height: 32px;
	margin: 20px 0 10px;
	left: -6px;
	position: relative;
}

.des-remark {
	color: #3070A0;
	font-size: 14px;
	padding: 5px;
	border-top: 1px #CCC solid;
}

.container .content-des {
	font-size: 14px;
	padding: 8px;
	border: 1px dashed #0097CF;
    border-radius: 4px;
	margin-top: 10px;
}

.des-1 {
	background-color: rgba(246, 255, 234, 0.6);
	*background-color: #F6FFEA;
}

.des-2 {
	background-color: rgba(255, 255, 215, 0.6);
	*background-color: #FFFFD7;
}

.des-3 {
	background-color: rgba(185, 224, 228, 0.6);
	*background-color: #B9E0E4;
}

.des-4 {
	background-color: rgba(154, 205, 245, 0.6);
	*background-color: #9ACDF5;
}

#ebank .logo img {
	width: 100px;
	padding: 5px 0;
}

.content-des ul {
	padding: 0 10px;
}

.container .content-des li {
	text-align: center;
	list-style: none;
	padding: 2px 8px;
    color: #008194;
	line-height: 150%;
	display: inline-block;
	zoom: 1;
	*display: inline;
}

.container .content-des, .container .content-des .remark {
	color: #008000;
}

.container .content-des, .container .content-des li b {
	color: #FF0000;
}

.content-title {
	font-size: 15px;
	padding-top: 10px;
	font-weight: bold;
}

.content-title span {
	margin-right: 50px;
	display: inline-block;
	*display: inline;
}

.container .pay-body {
	border: 1px solid #8AC0C7;
	border-top: 3px solid #008194;
    border-radius: 4px;
	overflow: hidden;
	margin: 10px 0 50px;
	background-color: rgba(255, 255, 255, 0.6);
	*background-color: #FFFFFF;
}

.container .pay-body .order h2 {
	line-height: 30px;
	height: 30px;
	padding-left: 10px;
	border-bottom: 1px solid #ccc;
}

.container .pay-body .order .search {
	padding: 8px 10px 3px;
	border-bottom: 1px solid #ccc;
}

.container .pay-body .order h3 {
	border-bottom: 1px dashed #8AC0C7;
	font-size: 18px;
	font-weight: bold;
	line-height: 30px;
	height: 30px;
	padding-left: 10px;
	margin: 0;
}

.container .pay-body .pay-body-content {
	padding: 10px;
}

.container .pay-body li, .center .center-right .content ul li {
	line-height: 0;
	display: inline-block;
	zoom: 1;
	*display: inline;
}

.container .pay-body ul.check {
	border-bottom: 1px solid #8AC0C7;
	padding: 5px 20px 0;
	background-color: #FBFBFB;
}

.container .pay-body ul.check li a {
	width: 70px;
	height: 35px;
	line-height: 35px;
	margin-right: 3px;
	border: 1px solid #8AC0C7;
	text-align: center;
	background-color: #E9F3F5;
	border-radius: 5px 5px 0 0;
	position: relative;
	bottom: -1px;
	*bottom: -2px;
}

.container .pay-body ul.check li a.active {
	height: 36px;
	line-height: 36px;
	border-top: 2px solid #0097CF;
	border-bottom: none;
	*bottom: -1px;
}

.container .pay-body ul.check li a.active, .container .pay-body ul.check li a:HOVER {
	background-color: #FFFFFF;
	color: #0097CF;
}

.container .pay-body li a {
	margin: 0px;
	display: inline-block;
	zoom: 1;
	color: #008194;
	*cursor: pointer;
	*display: inline;
}

.container .pay-body .pay-body-content li a {
	border: 1px solid #FFF;
	line-height: 170%;
	height: 50px;
	width: 220px;
	padding: 5px;
}

.container .pay-body .pay-body-content li a:HOVER {
	border: 1px solid #0097CF;
	background-color: #BEEBF1;
}

.pay-body-content li .logo {
	float: left;
	padding: 3px;
	width: 100px;
	text-align: center;
	margin-left: 15px;
}

.pay-body-content li .des {
	float: left;
	padding-left: 10px;
}

.pay-body-content li .des .ratio {
	display: block;
	color: #FF0000;
}

.container .pay-body .order ul, .container .pay-body .order dl {
	padding-left: 15px;
}

.container .pay-body .order table {
	border-collapse: collapse;
	border-spacing: 0;
	width: 100%;
	border: none;
}

.container .pay-body .order thead {
	background-color: #f0f4f6;
}

.container .pay-body .order tr {
	text-align: center;
}

.order td {
	border-top: 1px solid #ccc;
}

.container .pay-body .order .hder {
	padding: 10px 5px;
	border-left: 1px solid #ccc;
}

.info {
	margin-top: 80px;
	border: 1px solid #FFD0A8;
	color: #008194;
}

.info .exception {
	background: url(../images/error.png);
}

.info .exception strong {
	color: rgb(0, 0, 0);
	display: block;
	font-size: 18px;
	font-family: 黑体;
	font-weight: normal;
	line-height: 20px;
	margin-left: 65px;
	padding: 15px 0px 10px 3px;
}

.info .exception .Todo {
	color: #666;
	font-size: 12px;
	font-weight: normal;
	line-height: 18px;
	margin-left: 68px;
	padding: 0 0 0 3px;
}

.info .exception ul.error {
	border-top: 1px solid #CCC;
	margin-left: 68px;
	margin-right: 20px;
	margin-top: 5px;
	padding-left: 27px;
	padding-top: 8px;
}

.info .exception ul.error li {
	list-style: initial;
}

.info .exception {
	background-color: #FFE;
	background-position: 20px 20px;
	background-repeat: no-repeat;
	border: 1px solid #FFF0A8;
	font-size: 14px;
	height: auto;
	min-height: 60px;
	padding: 20px 20px 30px 20px;
	width: auto;
	font-family: Verdana, Tahoma, Arial, "Helvetica Neue", Helvetica, "微软雅黑";
}

.table {width: 100%;}
.table caption, .table th, .table td {
	padding: 8px;
	text-align: center;
	vertical-align: middle;
    word-wrap: break-word;
    word-break: break-all;
}
.table thead th {border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;}
.table td p {margin: 0px;}
.table thead tr {
    background: #f2f2f2;
    background-image: -webkit-gradient(linear,left 0,left 100%,from(#f8f8f8),to(#ececec));
    background-image: -webkit-linear-gradient(top,#f8f8f8,0%,#ececec,100%);
    background-image: -moz-linear-gradient(top,#f8f8f8 0,#ececec 100%);
    background-image: linear-gradient(to bottom,#f8f8f8 0,#ececec 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fff8f8f8',endColorstr='#ffececec',GradientType=0);
}

.table th span {
	font-size: 14px;
    float: left;
    height: 30px;
    line-height: 30px;
    margin-right: 5px;
}

.table tbody tr:nth-child(odd) td, .table tbody tr:nth-child(odd) th {
    background-color: #f9f9f9;
}

.table tbody tr:hover td, .table tbody tr:hover th {
    background-color: #f5f5f5;
}

.table caption, .table tfoot {background: #eff3f8;}

.foot {
	position: fixed;
	bottom: 0px;
	width: 100%;
	background-color: #008194;
	box-shadow: 4px 0 10px #517175;
	-moz-box-shadow: 4px 0 10px #517175;
	-webkit-box-shadow: 4px 0 10px #517175;
	/*IE position: fixed*/
	_position: absolute;
	_top: expression_r(documentElement.scrollTop +  
		 documentElement.clientHeight-this.offsetHeight);
	*filter: progid:DXImageTransform.Microsoft.Shadow(color='#517175',
		Direction=0, Strength=5);
}

.foot .content {
	color: #FFFFFF;
	text-align: center;
	font-size: 14px;
}

.foot .content p {
	margin: 10px 0;
}

.online-server {
	z-index: 2;
	position: fixed;
	top: 250px;
	right: 0px;
	background: #008194;
	border-radius: 5px 0 0 5px;
	box-shadow: -4px 3px 8px #517175;
	-moz-box-shadow: -4px 3px 8px #517175;
	-webkit-box-shadow: -4px 3px 8px #517175;
	*filter: progid:DXImageTransform.Microsoft.Shadow(color='#517175',
		Direction=225, Strength=6);
	/*IE position: fixed*/
	_position: absolute;
}

.online-server .float {
	width: 36px;
	float: left;
	position: relative;
	height: 200px;
	background: #008194;
	border-radius: 5px 0 0 5px;
}

.online-server .float .float-tools {
	position: relative;
	top: 32px;
	left: 0;
	background-image: url(../images/server.png);
	background-repeat: no-repeat;
	display: block;
	height: 136px;
	padding: 8px;
	cursor: pointer;
}

.tools-open {
	background-position: -37px 0;
}

.online-server .float-info {
	float: left;
	padding: 0 5px;
	overflow: hidden;
	background: #008194;
	display: none;
	border-radius: 0 0 0 5px;
}

.online-server .float-info .online {
	background: #F7F7F7;
	border-radius: 5px;
	margin: 15px 0;
	min-height: 162px;
	min-width: 185px;
	/*IE min-height、min-weidth*/
	_height: expression(( document.documentElement.clientHeight || 
		 document.body.clientHeight)>162?"162px":"");
	_width: expression(( document.documentElement.clientWidth || 
		 document.body.clientWidth)>185?"185px":"");
}

.online-server .float-info .online .title {
	font-size: 15px;
	color: #008194;
	font-weight: 600;
	line-height: 24px;
	padding: 5px;
	text-align: center;
	margin-bottom: 5px;
}

.online-server .float-info .online li {
	line-height: 38px;
	height: 38px;
	border-top: solid 1px #8AC0C7;
	overflow: hidden;
	text-align: left;
	padding: 0 10px;
}

.online-server .float-info .online li img {
	vertical-align: middle;
}

.ui-tooltip, .arrow:after {
	background-color: rgba(0, 129, 148, 0.9);
	background-color: #008194\9;
	border: 2px solid white;
}

.arrow.top:after {
	bottom: -20px;
	top: auto;
}

.arrow.top {
	top: -16px;
	bottom: auto;
}

.arrow.left {
	left: 20%;
}

.ui-tooltip {
	position: absolute;
	z-index: 99;
	max-width: 300px;
	padding: 10px 20px;
	color: white;
	border-radius: 4px;
	font-family: Verdana, Tahoma, Arial, "Helvetica Neue", Helvetica, "微软雅黑";
	text-transform: uppercase;
	box-shadow: 0 0 7px #008194;
	font-size: 1.2em;
}

.arrow:after {
	content: "";
	position: absolute;
	left: 20px;
	top: -20px;
	width: 25px;
	height: 25px;
	box-shadow: 6px 5px 9px -9px black;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}

.tool-tips {
	position: fixed;
	z-index: 999;
	bottom: 50%;
	left: 50%;
	margin: 0 0 -20px -80px;
	background-color: rgba(0, 0, 0, 0.25);
	*filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#60000000,
		endColorstr=#60000000);
	padding: 3px;
	border-radius: 3px;
	/*IE position: fixed*/
	_position: absolute;
}

.tool-tips-wrap {
	padding: 10px 20px;
	text-align: center;
	border: 1px solid #000000;
	background-color: rgba(0, 0, 0, 0.75);
	background-color: #000000\9;
	font-size: 14px;
	color: #fff;
	border-radius: 3px;
}

#qrcode-type {
	display: inline-block;
	width: 200px;
	height: 60px;
	background-image: url("../images/qrcodes.png");
}
.qrcode-wechat {background-position-x: -200px}
.qrcode-hbpay {background-position-x: -400px}
.qrcode-qpay {background-position-x: -600px}
.qrcode-tenpay {background-position-x: -800px}
.qrcode-union {background-position-x: -1000px}

@media ( max-width : 1080px) {
	.header .content, .container, .foot .content, .info, .content-title {
		width: 90%;
	}
}

#qrcode {
	border: none;
	width: 250px;
	height: 250px;
	overflow: hidden;
	background: #a6a6a6\9;
	background: rgba(0, 0, 0, 0.35);
	background-image: url('../images/loading2.gif');
	background-repeat: no-repeat;
	background-position: 75px 75px;
}
@media ( max-width : 680px) {
	.header .content, .foot .content {
		width: 100%;
	}
	.info {
		width: calc(100% - 20px);
		margin: 40px 10px 0 10px;
	}
	.container, .content-title {
		width: calc(100% - 20px)!important;
		margin: 0 10px!important;
		left: 0 !important;
	}
	.header .tools {
		margin-right: 10px;
	}
	.center div.center-left {
		width: 100%!important;
		border-bottom: 1px solid #D7E5FF;
	}
	.center div.center-right {
		width: 100%;
		min-height: inherit;
		border-left: none;
	}
	div.online-server {
	    top: 80px;
	}
	.online-server div.float {
		height: 38px;
	}
	.online-server .float a.float-tools {
		height: 21px;
		top: 8px;
	}
	a.tools-open {
		height: 120px!important;
		top: 32px!important;
	}
	.lab {
		width: auto;
	}
}

@media ( max-width : 468px) {
	.header .content {
		background-image: url("../images/web/logo-min.png");
		background-size: 105px 60px;
	}
}