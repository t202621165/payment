.page-header {
	position: absolute;
	z-index: 11;
	-moz-box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
	-webkit-box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
	box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
	*filter: progid:DXImageTransform.Microsoft.Shadow(color='#000',
		Direction=180, Strength=5);
}

.page-header-content {
	height: 60px;
	background-repeat: no-repeat;
	position: relative;
	*position: relative;
}

.page-heager-nav {
	display: block;
	overflow: hidden;
	height: 100%;
}

.page-heager-nav a img.logo {width: 270px;height: 60px;}

.page-heager-nav a img.cache {width: 105px;height: 60px;}

.page-heager-nav .menus {
	display: inline-block;
	width: calc(100% - 290px);
	margin-left: 20px;
}

.page-heager-nav .menus a.profile {
	float: right;
	margin: 0;
}

.page-heager-nav .menus a {
	color: #fff;
	font-weight: 500;
	font-size: 16px;
	height: 60px;
	line-height: 60px;
	display: inline-block;
	letter-spacing: 2px;
	padding: 0 10px;
	margin: 0 5px;
	float: left;
}

.page-content {
	background: #fff;
	border-radius: 3px;
	box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
	-webkit-box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
	position: relative;
	margin: 0 auto 30px !important;
	top: 80px;
	_top: 20px;
}

.page-content .control {
	position: absolute;
	height: 100%;
	left: 0px;
	_height: expression(document.getElementById ( 'page-content')
		.offsetHeight+"px");
	width: 160px;
	background: #f2f2f2;
	border-right: 1px solid #CCC;
	background-repeat: repeat-y;
	background: -webkit-linear-gradient(top, #ffffff 6%, #f5f5f5 30%, #e8e8e8 100%);
	background: -moz-linear-gradient(top, #ffffff 6%, #f5f5f5 30%, #e8e8e8 100%);
	background: -o-linear-gradient(top, #ffffff 6%, #f5f5f5 30%, #e8e8e8 100%);
	background: linear-gradient(to bottom, #ffffff 6%, #f5f5f5 30%, #e8e8e8 100%);
	background-repeat: repeat-x;
	filter: progid: DXImageTransform.Microsoft.gradient(startColorstr=#ffffff,
		endColorstr=#e8e8e8, GradientType=0);
}

.page-content .control .title-bar {
	font-size: 16px;
	font-weight: bold;
	padding: 0 15px;
	color: #fff;
	height: 40px;
	line-height: 40px;
	letter-spacing: 3px;
	text-align: center;
}

