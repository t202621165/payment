}

.page-content .control .wrapper li a {
	background-image: url(../images/mer/icons.png);
	padding-left: 40px;
	background-repeat: no-repeat;
	background-position-x: left;
	height: 32px;
	display: block;
	line-height: 32px;
}

a.link {
	color: #2a8bcc;
}

.page-content .control .wrapper a:hover, a.line:hover, a.link:hover {
