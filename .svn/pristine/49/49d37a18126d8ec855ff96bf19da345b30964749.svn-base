    padding: 10px 20px;
    margin-top: 10px;
    color: #008194;
    position: relative;
}

#take-money {
	margin: 30px auto 0;
    width: 500px;
}

#take-money th, #take-money td {
	padding: 10px;
}
#take-money th {
	text-align: right;
}

.col-total {
	background: #d9edf7;
	margin-bottom: 10px;
	border-radius: 4px;
}

.table-total {
	width: 100%;
}

.table-total th {
	color: #008194;
}
.table-total td {
	padding: 5px;
	text-align: center;
}
.table-total .yesterday {
	color: #888888;
}
.table-total .terday {
	color: #468847;
	font-weight: bold;
}

ul#notice {
	height: 30px;
	padding-left: 45px;
	overflow: hidden;
}
ul#notice li {
	padding: 6px 0;
}

ul#notice a {
	color: red;
}

ul#notice a:hover {
	text-decoration: underline;
}

.template-pro {
	float: left;
	padding: 5px 10px 5px 0;
	width: 150px;
	text-align: right;
	height: 40px;
	line-height: 40px;
}

.changePattern {
    position: fixed;
    padding: 0 5px 8px;
    background: #000;
    top: 200px;
    border-radius: 0 3px 3px 0;
}

.changePattern span {
	height: 20px;
    width: 20px;
    margin-top: 8px;
    display: inherit;
    border-radius: 3px;
    cursor: pointer;
}

.changePattern span.default {
    background: #008194;
}

.wechat-icon {
	border: solid 1px #d9edf7;
	width: 36px;
	height: 36px;
	border-radius: 50%;
	margin: 2px 0;
	cursor: pointer;
}
.wechat-icon:hover {
	border: solid 1px #78c6ec;
}

.binding-title {
	height: 40px;
	padding: 5px 0;
	float: left;
	margin-left: 25px;
}

.binding-info {
	line-height: 40px;
	height: 40px;
	padding: 0 10px 0 0;
}

@media ( min-width : 1030px) {
	.page-header {
		position: fixed;
		width: 100%;
	}
}

@media ( max-width : 1280px) {
	.col-sm-2 {width: 66.6666%;}
	.col-xs-4 {width: 33.3333%;}
	.hide-1280 {
		display: none!important;
	}
	.node-info-name {text-align: left!important;width: 100%!important;}
	.node-info-name.none{display: none;}
	.node-body .node-info-value {
	    width:100%!important;
	}
}

@media ( max-width : 1200px) {
	.hide-1200 {
		display: none;
	}
	.show-1200 {
		display: block;
	}
}

@media ( max-width : 1080px) {
	.hide-1080 {
		display: none;
	}
}

@media ( max-width : 980px) {
	.show-980 {
		display: block;
	}
	img.show-980 {
		display: initial;
	}
	.hide-980 {
		display: none!important;
	}
	.col-sm-2 {width: 100%;}
	div.pull-980 {width: 100%;}
}

@media ( max-width : 860px) {
	.node-info {margin-bottom: 0!important;}
	.hide-860 {
		display: none!important;
	}
	.show-860 {
		display: block!important;
	}
}

@media ( max-width : 768px) {
	.page-heager-nav a img.logo {
		display: none;
	}
	.page-heager-nav a img.cache {
		display: block !important;
	}
	.page-heager-nav .menus {
	    display: inline-block;
	    width: calc(100% - 120px);
	    margin-left: 15px;
	}
	.hide-768 {
		display: none!important;
	}
	.show-768 {
		display: block!important;
	}
	.btn-list.show-768 {
		display: inline-block!important;
	}
}

@media ( max-width : 680px) {
	.hide-680 {
		display: none!important;
	}
}

@media ( max-width : 520px) {
	.col-xs-2 {width: 100%;}
}