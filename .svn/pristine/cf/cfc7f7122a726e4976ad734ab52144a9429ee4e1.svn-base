/*label-checkbox*/
.box-nomargin .label-box {
    margin-right: 10px;
}
.label-box {
	cursor: pointer;
	display: inline-block;
    position: relative;
    -moz-user-select:none;
    -webkit-user-select:none;
    -ms-user-select:none;
    -khtml-user-select:none;
	user-select:none;
}
.label-box.checkbox-min {
	margin: 3px 10px 3px 0;
}

.label-box.checkbox:hover > :not(input) {
	background-color: #c2c2c2;
}

.label-box.checkbox:hover > i {
	border-color: #c2c2c2;
	color: #c2c2c2;
}

.label-box input, input.input-box {
	display: none;
	display: block\9;
}

.label-box.checkbox input {
	position: absolute\9;
    right: -4px\9;
    top: 5px\9;
}

.label-box.checkbox.checkbox-min input {
	position: absolute\9;
    right: -7px\9;
    top: 2px\9;
}

.label-box.checkbox2 input {
	position: relative\9;
    top: 4px\9;
    left: 5px\9;
}

.label-box.checkbox input:checked ~ *, .label-box.checkbox2 input:checked ~ i {
	background-color: #00B066;
}

.label-box.checkbox.box-blue input:checked ~ *, .label-box.checkbox2.box-blue input:checked ~ i {
	background-color: #2a80d5;
}

.label-box.checkbox input:checked ~ *, 
.label-box.checkbox2 input:checked ~ i, 
.label-box .box-switch i:after, 
.label-box .box-switch {
    transition: all ease-out .3s;
    -moz-transition: all ease-out .3s;
	-webkit-transition: all ease-out .3s;
	-o-transition: all ease-out .3s;
}

.label-box.checkbox:hover > i.check, .label-box.checkbox input:checked ~ i.check {
    background-color: #FFF!important;
}

.label-box.checkbox input:checked ~ i, .input-box:checked ~ .label-box i {
    border-color: #00B066;
}

.label-box.checkbox.box-blue input:checked ~ i, .input-box:checked ~ .label-box.box-blue i {
    border-color: #2a80d5;
}

.label-box.checkbox:hover > i.default, .label-box.checkbox input:checked ~ i.default {
	box-shadow: 0px 0px 0px 6px #ffffff inset;
	-moz-box-shadow: 0px 0px 0px 6px #ffffff inset;
	-webkit-box-shadow: 0px 0px 0px 6px #ffffff inset;
}

.label-box.checkbox:hover > i.check:after, .label-box input:checked ~ i.check:after, 
.input-box:checked ~ .label-box i.check:after {
    content: "✔";
    position: absolute;
}

.label-box.checkbox2 input:checked ~ i.check:after {
	top: -1px;
    left: -1px;
    height: 16px;
    line-height: 16px;
}

.label-box.checkbox:hover > i.check:after, .label-box.checkbox input:checked ~ i.check:after, 
.input-box:checked ~ .label-box i.check:after {
    top: 0px;
    line-height: 24px;
}

.label-box.checkbox input:checked ~ i.check:after, .input-box:checked ~ .label-box i.check:after {
    color: #00B066;
    line-height: 24px;
}

.label-box.checkbox.box-blue input:checked ~ i.check:after, .input-box:checked ~ .label-box.box-blue i.check:after {
    color: #2a80d5;
}

