.progress {
	height: 12px;
    background: #dadada;
    margin: 1px 0;
    position: relative;
}

.progress:after {
	content: attr(data-percent);
	text-align: center;
	color: #fff;
	position: absolute;
	width: 100%;
	top: -2px;
    height: 100%;
    font-size: 13px;
}

.progress-bar {
    width: 0;
	height: 100%;
	background: #00b066;
	background-image: -webkit-gradient(linear,0 100%,100% 0,color-stop(0.25,rgba(255,255,255,0.2)),color-stop(0.25,transparent),color-stop(0.5,transparent),color-stop(0.5,rgba(255,255,255,0.2)),color-stop(0.75,rgba(255,255,255,0.15)),color-stop(0.75,transparent),to(transparent));
    background-image: -webkit-linear-gradient(45deg,rgba(255,255,255,0.2) 25%,transparent 25%,transparent 50%,rgba(255,255,255,0.2) 50%,rgba(255,255,255,0.2) 75%,transparent 75%,transparent);
    background-image: -moz-linear-gradient(45deg,rgba(255,255,255,0.2) 25%,transparent 25%,transparent 50%,rgba(255,255,255,0.2) 50%,rgba(255,255,255,0.2) 75%,transparent 75%,transparent);
    background-image: linear-gradient(45deg,rgba(255,255,255,0.2) 25%,transparent 25%,transparent 50%,rgba(255,255,255,0.2) 50%,rgba(255,255,255,0.2) 75%,transparent 75%,transparent);
    background-size: 40px 40px;
    transition: all ease-out .3s;
    -moz-transition: all ease-out .3s;
	-webkit-transition: all ease-out .3s;
	-o-transition: all ease-out .3s;
}

.ui-tooltip, .ui-tooltip .arrow:after {
	background-color: rgba(0, 129, 148, 0.9);
	*background-color: #008194;
	border: 2px solid white;
}

.ui-tooltip .arrow {
	width: 70px;
	height: 16px;
	overflow: hidden;
	position: absolute;
	left: 50%;
	margin-left: -35px;
	bottom: -16px;
}

.ui-tooltip .arrow.top:after {
	bottom: -20px;
	top: auto;
}

.ui-tooltip .arrow.top {
	top: -16px;
	bottom: auto;
}

.ui-tooltip {
	position: absolute;
	z-index: 99;
	max-width: 300px;
	padding: 10px 20px;
	color: white;
	border-radius: 4px;
	font-family: Verdana, Tahoma, Arial, "Helvetica Neue", Helvetica, "微软雅黑";
	text-transform: uppercase;
	box-shadow: 0 0 7px #008194;
	font-size: 1.2em;
}

.ui-tooltip .arrow:after {
	content: "";
	position: absolute;
	left: 20px;
	top: -20px;
	width: 25px;
	height: 25px;
	box-shadow: 6px 5px 9px -9px black;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	transform: rotate(45deg);
}