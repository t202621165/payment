.lable {
	height: 20px;
	line-height: 20px;
	text-align: center;
	background: #00b066;
	color: #fff;
    white-space: nowrap;
    display: inline-block;
    position: relative;
    z-index: 1;
    padding: 0 5px;
}

.lable.gray {
	background: #607D8B!important;
}

.lable.green {
	background: #009688!important;
}

.lable.orange {
	background: #FF5722!important;
}

.lable.danger {
	background: #ef3821!important;
}

.lable.primary {
	background: #2a8bcc!important;
}

.lable:before {
    border: 1px solid transparent;
    border-right-color: #00b066;
	left: -10px;
}

.lable.lable-right:after {
    border: 1px solid #00b066;
    border-right-color: transparent;
}

.lable.green.lable-right:after {
    border: 1px solid #009688;
    border-width: 10px 5px;
    border-right-color: transparent;
}

.lable.orange.lable-right:after {
    border: 1px solid #FF5722;
    border-width: 10px 5px;
    border-right-color: transparent;
}

.lable.gray.lable-right:after {
    border: 1px solid #607D8B;
    border-width: 10px 5px;
    border-right-color: transparent;
}

.lable.danger.lable-right:after {
    border: 1px solid #ef3821;
    border-width: 10px 5px;
    border-right-color: transparent;
}

.lable.primary.lable-right:after {
    border: 1px solid #2a8bcc;
    border-width: 10px 5px;
    border-right-color: transparent;
}

.lable:before, .lable.lable-right:after {
	display: inline-block;
    content: "";
    position: absolute;
    top: 0;
    z-index: -1;
    border-width: 10px 5px;
}

.lable.orange:before {
    border-right-color: #FF5722!important;
}

.lable.green:before {
    border-right-color: #009688!important;
}

.lable.gray:before {
    border-right-color: #607D8B!important;
}

.lable.danger:before {
    border-right-color: #ef3821!important;
}

.lable.primary:before {
