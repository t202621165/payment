body, .table caption, .table tfoot {
	background: #383a43;
}
body, a {
	color: #bdcadb;
}
.box-info {
    background: #1c1f2b;
}
.dot-line {
    border-top: 1px dotted #f3eacb;
}
.table tbody tr:nth-child(odd) td, .table tbody tr:nth-child(odd) th {
    background-color: #252737;
}
.table tbody tr:hover td, .table tbody tr:hover th {
    background-color: #303241;
}
.form-control, #pageSize {
    color: #aecadb;
    border: solid 1px #a37419;
    background: #252737;
}
.table thead th {
    border-top: 1px solid #000;
    border-bottom: 1px solid #000;
}
.page-content .control {
    background: #383c49;
    background: -webkit-linear-gradient(top, #383c49 6%, #313647 30%, #1c1f2b 100%);
    background: -moz-linear-gradient(top, #383c49 6%, #313647 30%, #1c1f2b 100%);
    background: -o-linear-gradient(top, #383c49 6%, #313647 30%, #1c1f2b 100%);
    background: linear-gradient(to bottom, #383c49 6%, #313647 30%, #1c1f2b 100%);
    background-repeat: repeat-x;
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr=#383c49,
 endColorstr=#1c1f2b, GradientType=0);
}
.table thead tr {
    background: #19191c;
    background-image: -webkit-gradient(linear,left 0,left 100%,from(#595b68),to(#19191c));
    background-image: -webkit-linear-gradient(top,#595b68 0%,#19191c 100%);
    background-image: -moz-linear-gradient(top,#595b68 0,#19191c 100%);
    background-image: linear-gradient(to bottom,#595b68 0,#19191c 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#595b68',endColorstr='#19191c',GradientType=0);
}
.form-heading {
    border-bottom: 1px solid #111;
    background-color: #383a43;
}
.form-heading ul li.active, .form-heading ul li:hover {
    background-color: #111;
    border-right: 1px solid #111;
    border-top: 1px solid #111;
    border-left: 1px solid #111;
}
.form-heading ul li {
    border: 1px solid #383a43;
    border-bottom: 1px solid transparent;
}
.content-body ul.nav {
    border-bottom: 1px solid #383a43;
    background-color: #111;
}
.content-body ul.nav li a.active {
    border-top: 2px solid #d49009!important;
}
.content-body ul.nav li a.tab {
    border: 1px solid #383a43;
    background-color: #383a43;
}
.content-body ul.nav li a.active, .content-body ul.nav li a.tab:hover {
    background-color: #111111;
    color: #d49009;
}
.col-total {
    background: #f3eacb;
}
.table-total th {
    color: #704214;
}
.page-header, .page-content .control .title-bar, .page-content .content 
.content-title, .modal-box {
	background-color: #2d3143;
}


.table th, .font-default, .info, .content-body ul.nav li a, 
.page-content .control .wrapper li a, .profile-title, .modal-box-body, 
.dropdown-menu li a, .form-heading ul li a {
	color: #c1c6d7;
}

.modal-box, .modal-box-body {
	border-color: #2d3143;
}
.user-col1, ul.user-col .vline, .box-info, .user-accounts, .option-box li, .bank-info {
	border-color: #877f6a;
}

.user-accounts a, a.line.link {
	color: #FFFFFF;
}

.page-heager-nav .menus a.on, .page-heager-nav .menus a:hover, .page-content .content {
	background-color: #1c1f2b;
}

.btn:not(.disable):hover, .btn:not([disabled]):hover {
	background-color: #0193a9;
}

.modal-box {
    background: -webkit-linear-gradient(top,#008194 0,#00BCD4 30%);
    background: -moz-linear-gradient(top,#008194 0,#00BCD4 30%);
    background: -o-linear-gradient(top,#008194 0,#00BCD4 30%);
    background: linear-gradient(to bottom,#008194 0,#00BCD4 30%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#008194,endColorstr=#00BCD4,GradientType=0);
}