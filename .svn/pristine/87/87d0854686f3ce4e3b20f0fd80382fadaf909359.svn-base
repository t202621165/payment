<html xmlns:th="http://www.thymeleaf.org"
	xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>product.html</title>
	
    <meta name="keywords" content="keyword1,keyword2,keyword3">
    <meta name="description" content="this is my page">
    <meta name="content-type" content="text/html; charset=UTF-8">
    
    <link th:href="@{/assets/css/css.css}" rel="stylesheet" type="text/css">
	<link th:href="@{/assets/css/main.css}" rel="stylesheet" type="text/css">

  </head>
  <div th:replace="manage/common/common_js :: basic"></div>
  <body>
    	<div :controller="galleryFunds">
    		<table th:width="100+'%'" th:border="0" th:cellspacing="0"
					th:cellpadding="0" id="main-tab">
				<tbody th:class="@{active-tb baseInfo}">
				<tr>
					<td colspan="4" th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'><h3>通道收款详情</h3></td>
				</tr>
				<tr onmouseout="style.backgroundColor='#ffffff'"
					onMouseOver="this.style.backgroundColor='#edf5ff'">
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>支付宝</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'alipay'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>微信</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'wechat'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>						
					</td>
				</tr>
				<tr onmouseout="style.backgroundColor='#ffffff'"
					onMouseOver="this.style.backgroundColor='#edf5ff'">
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>网银</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'ebank'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>蚂蚁花呗</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'hbpay'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
				</tr>
				<tr onmouseout="style.backgroundColor='#ffffff'"
					onMouseOver="this.style.backgroundColor='#edf5ff'">
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>QQ钱包</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'qpay'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>财付通</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'tenpay'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
				</tr>

				<tr onmouseout="style.backgroundColor='#ffffff'"
					onMouseOver="this.style.backgroundColor='#edf5ff'">
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom bggray font-weight}'>银联扫码</td>
					<td th:align='center' th:valign='middle'
						th:class='@{borderright borderbottom main-for}'>
						<span :if="item.mark == 'union'" :text="@item.amount | number(2)" :for="($index,item) in @dup" style="font-weight: bold;color:green">0.00</span>
					</td>
					<td colspan="2"></td>
				</tr>
			</tbody>
			</table>
    	</div>
    	<div th:replace="manage/common/common_js :: work"></div>
    	<script th:inline="javascript">
    		var options = {
    			id:{
    				id:"galleryFunds"
    			}
    		}
    		var vm = work(options);
    		vm.dup = [[${galleryFunds}]];
    	</script>
  </body>
</html>
