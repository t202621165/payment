package com.cypay.common.to;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

/**
 * 游戏引擎
 * @author International
 *
 */
public class GameEngine implements Serializable {

	private static final long serialVersionUID = 1L;
	
	public static List<GameEngine> engines = new ArrayList<GameEngine>();

	/**游戏引擎*/
	private String engine;
	
	/**浏览器指令*/
	private String command;
	
	static {
		engines.add(new GameEngine("LEG", "WebBrowser"));
		engines.add(new GameEngine("GEE", "OpenWebSite"));
		engines.add(new GameEngine("3KM2", "WebBrowser"));
		engines.add(new GameEngine("MaxM2", "WebBrowser"));
		engines.add(new GameEngine("Bluem2", "WebBrowser"));
		engines.add(new GameEngine("Herom2", "OpenWebSite"));
		engines.add(new GameEngine("BLUE2017", "WebBrowser"));
		engines.add(new GameEngine("Gameofmir", "OpenWebSite"));
	}
	
	public GameEngine() {
		
	}
	
	public GameEngine(String engine, String command) {
		this.engine = engine;
		this.command = command;
	}

	public String getEngine() {
		return engine;
	}

	public void setEngine(String engine) {
		this.engine = engine;
	}

	public String getCommand() {
		return command;
	}

	public void setCommand(String command) {
		this.command = command;
	}

	@Override
	public String toString() {
		return "GameEngine [engine=" + engine + ", command=" + command + "]";
	}
	
}
