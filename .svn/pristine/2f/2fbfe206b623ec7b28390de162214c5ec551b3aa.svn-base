.shortcut-box .icon-up .icon-angle-up:after{width: 3px; height: 27px; clear: both; content: ""; display: block; background: #FFFFFF; -webkit-transform: rotate(-225deg); -moz-transform: rotate(-225deg); -ms-transform: rotate(-225deg); -o-transform: rotate(-225deg); transform: rotate(-225deg); -webkit-transition: 0.35s; -moz-transition: 0.35s; -ms-transition: 0.35s; -o-transition: 0.35s; transition: 0.35s; position: absolute; left: 65%; top: 12px;}
.shortcut-box .icon-qq{background: url(/assets/images/web/icon.png); *zoom: 1; *display: inline; display: inline-block; width: 34px; height: 52px; background-position: -279px 8px; display: block; margin: 0 auto;}
.shortcut-box .icon-ewm{background: url(/assets/images/web/icon.png); *zoom: 1; *display: inline; display: inline-block; width: 34px; height: 52px; background-position: -318px 8px; display: block; margin: 0 auto;}
