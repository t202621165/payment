/*登录*/
.login-form{-webkit-transform: scale(0); -moz-transform: scale(0); -ms-transform: scale(0); -o-transform: scale(0); transform: scale(0); -webkit-transition: 0.3s; -moz-transition: 0.3s; -ms-transition: 0.3s; -o-transition: 0.3s; transition: 0.3s; opacity: 0; overflow: hidden; width: 390px; height: 380px; background:url(/assets/images/web/lg-bg.png) repeat 0 0; padding: 30px 50px 30px; box-shadow: 0 0 10px -3px #e5e5e5; /*position: fixed; z-index: 1033; top: 50%; left: 50%; margin: -120px 0 0 -225px;*/ border-radius: 5px;/*margin-left: 30px;*/}
.login-form.selected{-webkit-transform: scale(1); -moz-transform: scale(1); -ms-transform: scale(1); -o-transform: scale(1); transform: scale(1); -webkit-transition: 0.3s; -moz-transition: 0.3s; -ms-transition: 0.3s; -o-transition: 0.3s; transition: 0.3s; opacity: 1;}
.login-form:after{width: 100%; height: 4px; background: #049FF1; position: absolute; top: 0; left: 0; clear: both; content: ""; display: block;}
.login-form h3{color: #fff; font-size: 24px; margin-bottom: 25px; text-align: center;}
.login-form li{margin-bottom: 23px;}
.login-form li h1{display: block; float: left; width: 38px; height: 38px; background: #049FF1; text-align: center; border-bottom-left-radius: 2px; border-top-left-radius: 2px;}
.login-form li input, .form-btn{display: block; float: left; height: 38px; line-height: 38px; width: 250px; padding-left: 10px; border: 1px solid #e5e5e5; border-bottom-right-radius: 2px; border-top-right-radius: 2px; outline: none;}
.login-form li i{background: url(/assets/images/web/icon.png); *zoom: 1; *display: inline; display: inline-block; width: 25px; height: 28px}
.login-form li .i1{background-position: -295px -51px;}
.login-form li .i2{background-position: -325px -51px;}
.login-form li .i3{background-position: -353px -51px;}
.login-form #checkCode{height: 38px; line-height: 38px; border: 1px solid #e5e5e5; width: 80px;}
.login-form li.code-li input{width: 160px; margin-right: 10px;}
.login-form .btn{height: 40px; line-height: 40px; margin-top: 30px; width: 140px; background: #fa5403; border: 0; color: #FFFFFF;}
.login-form i.icon-colse{position: absolute; right: 0; top: 15px;}
.login-form i.icon-colse:before,
.login-form i.icon-colse:after{background: #333333;}
.in-ico{ color:#676566;}
.in-ico h3{ height:50px; line-height:50px}
.in-ico p{ font-size:14px; line-height:24px;}
/*tooltip*/
.tooltip-msg {
	z-index: 99;
	color: #FFF;
	font-size: 12px;
	*filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#60000000,
		endColorstr=#60000000);
	border-radius: 3px;
	/*IE position: fixed*/
	_position: absolute;
	position: fixed;
	top: 50%;
	left: 50%;
	margin: -20px 0 0 -72px;
	_position: absolute;
}

.tooltip-msg .tooltip-content {
	padding: 10px 20px;
	font-size: 15px;
	background-color: rgba(0, 0, 0, 0.8);
	*background-color: #000000;
	border-radius: 3px;
}

.tooltip-msg.danger .tooltip-content {
	border: 1px solid #992812;
    background: rgba(153,40,18,0.9);
	*background-color: #992812;
}

