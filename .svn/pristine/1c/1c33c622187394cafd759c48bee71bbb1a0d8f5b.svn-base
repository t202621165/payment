package com.cypay.common.enums;

/**
 * 商户角色枚举
 * 
 * @author International 2018年8月8日 下午3:49:01
 */
public enum MerchantRolesEnum {
	
	T0("T0商户", "t0"),
	
	T1("T1商户", "t1"),
	
	/** 代理商户 */
	agency("代理商户", "agency"),

	/** 普通商户 */
	merchant("普通商户", "merchant");

	private String name;
	
	private String mark;

	private MerchantRolesEnum(String name, String mark) {
		this.name = name;
		this.mark = mark;
	}

	public String getName() {
		return name;
	}

	public String getMark() {
		return mark;
	}

}
