/*label-checkbox*/
.box-nomargin .label-box {
    margin-right: 10px;
}
.label-box {
	cursor: pointer;
	display: inline-block;
    position: relative;
    -moz-user-select:none;
    -webkit-user-select:none;
    -ms-user-select:none;
    -khtml-user-select:none;
	user-select:none;
}
.label-box.checkbox-min {
	margin: 3px 10px 3px 0;
}

.label-box.checkbox:hover > :not(input) {
	background-color: #c2c2c2;
}

.label-box.checkbox:hover > i {
	border-color: #c2c2c2;
	color: #c2c2c2;
}

.label-box input, input.input-box {
	display: none;
	display: block\9;
}

.label-box.checkbox input {
	position: absolute\9;
    right: -4px\9;
    top: 5px\9;
}

.label-box.checkbox.checkbox-min input {
	position: absolute\9;
    right: -7px\9;
    top: 2px\9;
}

.label-box.checkbox2 input {
	position: relative\9;
    top: 4px\9;
    left: 5px\9;
}

.label-box.checkbox input:checked ~ *, .label-box.checkbox2 input:checked ~ i {
	background-color: #00B066;
}

.label-box.checkbox.box-blue input:checked ~ *, .label-box.checkbox2.box-blue input:checked ~ i, .label-box.checkbox2.box-blue i.box-checked {
	background-color: #2a80d5;
}

.label-box.checkbox input:checked ~ *, 
.label-box.checkbox2 input:checked ~ i, 
.label-box .box-switch i:after, 
.label-box .box-switch {
    transition: all ease-out .3s;
    -moz-transition: all ease-out .3s;
	-webkit-transition: all ease-out .3s;
	-o-transition: all ease-out .3s;
}

.label-box.checkbox:hover > i.check, .label-box.checkbox input:checked ~ i.check {
    background-color: #FFF!important;
}

.label-box.checkbox input:checked ~ i, .input-box:checked ~ .label-box i {
    border-color: #00B066;
}

.label-box.checkbox.box-blue input:checked ~ i, .input-box:checked ~ .label-box.box-blue i {
    border-color: #2a80d5;
}

.label-box.checkbox:hover > i.default, .label-box.checkbox input:checked ~ i.default {
	box-shadow: 0px 0px 0px 6px #ffffff inset;
	-moz-box-shadow: 0px 0px 0px 6px #ffffff inset;
	-webkit-box-shadow: 0px 0px 0px 6px #ffffff inset;
}

.label-box.checkbox:hover > i.check:after, .label-box input:checked ~ i.check:after, 
.input-box:checked ~ .label-box i.check:after {
    content: "✔";
    position: absolute;
}

.label-box.checkbox2 input:checked ~ i.check:after {
	top: -1px;
    left: -1px;
    height: 16px;
    line-height: 16px;
}

.label-box.checkbox:hover > i.check:after, .label-box.checkbox input:checked ~ i.check:after, 
.input-box:checked ~ .label-box i.check:after {
    top: 0px;
    line-height: 24px;
    right: 8px;
}

.label-box.checkbox input:checked ~ i.check:after, .input-box:checked ~ .label-box i.check:after {
    color: #00B066;
    line-height: 24px;
}

.label-box.checkbox.box-blue input:checked ~ i.check:after, .input-box:checked ~ .label-box.box-blue i.check:after {
    color: #2a80d5;
}

.label-box.checkbox span {
    padding: 0px 8px;
    height: 30px;
    line-height: 30px;
    font-size: 15px;
    background-color: #d2d2d2;
    color: #fff;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    float: left;
    vertical-align: middle;
}

.label-box.checkbox span.box-right {
    float: right!important;
}

.label-box.checkbox.checkbox-min span {
    padding: 0px 7px;
    height: 24px;
    line-height: 24px;
}

.label-box.checkbox span.box-left {
    border-radius: 2px 0 0 2px;
}

.label-box.checkbox span.box-right {
    border-radius: 0 2px 2px 0;
}

.label-box.checkbox i {
    width: 30px;
    float: left;
    height: 30px;
    _height: 28px;
    border: 1px solid #d2d2d2;
    background: #FFFFFF;
	font-size: 26px;
}

.label-input i {
	border-radius: 0 4px 4px 0!important;
}

.input-box:checked ~ .check-input {
	border: solid 1px #00b066;
}

.label-box.checkbox-min i {
	font-size: 20px;
}

.label-box.checkbox.checkbox-min i {
    width: 24px;
    float: left;
    height: 24px;
    _font-size: 0;
    _height: 22px!important;
}

.label-box.checkbox i.check-left {
    border-right: none;
    border-radius: 2px 0 0 2px;
}

.label-box.checkbox i.check-right {
    border-left: none;
    border-radius: 0 2px 2px 0;
}

.label-box.checkbox2 {
	height: 30px;
    line-height: 30px;
}

.label-box.checkbox2 input:checked ~ i {
    border-color: #00B066;
}

.label-box.checkbox2.box-blue input:checked ~ i, .label-box.checkbox2.box-blue i.box-checked {
    border-color: #2a80d5;
}

.label-box.checkbox2 input:checked ~ i.default, .label-box.checkbox2 i.box-checked {
	box-shadow: 0px 0px 0px 3px #ffffff inset;
	-moz-box-shadow: 0px 0px 0px 3px #ffffff inset;
	-webkit-box-shadow: 0px 0px 0px 3px #ffffff inset;
}

.label-box.checkbox2 i {
    display: inline-block;
	_display: none;
    position: relative;
    width: 16px;
    height: 16px;
    _font-size: 0;
    background: #FFFFFF;
    border: 1px solid #979797;
    border-radius: 2px;
    vertical-align: -2px;
    margin-right: 5px;
    color: #FFF;
}

.label-box.checkbox2 i.radio {
    width: 18px;
    height: 18px;
    border-radius: 50%;
    vertical-align: -4px;
}

.label-box .box-switch.disable {
	opacity: 0.6;
	cursor: not-allowed;
}
.label-box .box-switch {
	display: inline-block;
	_display: block;
	position: relative;
    height: 24px;
    font-size: 14px;
    _height: 0;
    line-height: 22px;
    _width: 0px;
    width: 60px;
    padding: 0 5px;
    border: 1px solid #ef3821;
    _border: 0;
    border-radius: 20px;
    cursor: pointer;
    background-color: #ef3821;
    -webkit-box-shadow: inset 0 2px 2px 0 rgba(0,0,0,.2);
    box-shadow: inset 0 2px 2px 0 rgba(0,0,0,.2);
}

.label-box .enable {
    border: 1px solid #d0cece;
    background-color: #d0cece;
}

.label-box input:checked ~ .box-switch {
	border-color: #00b066;
 	background: #00b066;
}
.label-box input:checked ~ .box-switch:after {
	content: attr(data-on);
	padding: 0 20px 0 2px;
 	color: #fff;
}
.label-box input:checked ~ .box-switch.dis:after {
	content: "\542f\7528";
}
.label-box input:checked ~ .box-switch i:after {
    left: 37px;
	background: #fff;
}
.box-switch:after {
	content: attr(data-off);
	font-size: 13px;
	padding: 0 2px 0 20px;
 	color: #fff;
}
.box-switch.dis:after {
	content: "\7981\7528";
}
.label-box .box-switch i:after {
	content: "";
	width: 18px;
	height: 18px;
	background: #fff;
    position: absolute;
    border-radius: 50%;
    top: 2px;
    left: 4px;
    -webkit-box-shadow: 0 1px 1px 1px rgba(0,0,0,.3);
    box-shadow: 0 1px 1px 1px rgba(0,0,0,.3);
}
.check-input {
	float: left;
	border-radius: 4px 0 0 4px!important;
}

.label-box input:checked ~ .box-switch.swith-blue {
	border-color: #2a80d5;
 	background: #2a80d5;
}

.label-box input:disabled ~ * {
	opacity: 0.5;
}
