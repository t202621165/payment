}
.animation{ 
   	font-size: 15px; 
   	color:#CC0033; 
   	margin: 10px;
   	animation: changeshadow 1s  ease-in  infinite ;
   	/* 其它浏览器兼容性前缀 */
    -webkit-animation: changeshadow 1s linear infinite;
    -moz-animation: changeshadow 1s linear infinite;
    -ms-animation: changeshadow 1s linear infinite;
    -o-animation: changeshadow 1s linear infinite;
}  
@keyframes changeshadow {  
    0%{ text-shadow: 0 0 4px #CC0033}  
    50%{ text-shadow: 0 0 40px #CC0033}  
    100%{ text-shadow: 0 0 4px #CC0033}  
}
   /* 添加兼容性前缀 */
@-webkit-keyframes changeshadow {
  0%{ text-shadow: 0 0 4px #CC0033}  
         50%{ text-shadow: 0 0 40px #CC0033}  
         100%{ text-shadow: 0 0 4px #CC0033}  
}
@-moz-keyframes changeshadow {
    0%{ text-shadow: 0 0 4px #CC0033}  
           50%{ text-shadow: 0 0 40px #CC0033}  
           100%{ text-shadow: 0 0 4px #CC0033}  
}
@-ms-keyframes changeshadow {
    0%{ text-shadow: 0 0 4px #CC0033}  
           50%{ text-shadow: 0 0 40px #CC0033}  
           100%{ text-shadow: 0 0 4px #CC0033}  
}
@-o-keyframes changeshadow {
    0%{ text-shadow: 0 0 4px #CC0033} 
   50%{text-shadow: 0 0 40px #CC0033} 
  100%{ text-shadow: 0 0 4px #CC0033}  
}


.art {
   font-size:18px; 
   font-weight:bold;
   color:#528fc6; 
   padding:15px 0 0 0;
   font-family: "Georgia";
   background: -webkit-linear-gradient(45deg, #528fc6, #666699, #336699);
   -moz-linear-gradient(45deg, #528fc6, #666699, #336699);
   -ms-linear-gradient(45deg, #528fc6, #666699, #336699);
   color: transparent;
   /*设置字体颜色透明*/
   -webkit-background-clip: text;
   /*背景裁剪为文本形式*/
   animation: ran 10s linear infinite;
   /*动态10s展示*/
}

@keyframes ran {
    from {
        backgroud-position: 0 0;
    }
    to {
        background-position: 2000px 0;
    }
}

